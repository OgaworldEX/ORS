<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
      <Console name="Console" target="SYSTEM_OUT">
         <PatternLayout pattern="%msg%n"/>
      </Console>
  </Appenders>
  <Loggers>
        <Root level="fatal">
          <AppenderRef ref="Console"/>
        </Root>
<!--

        <Root level="error">
          <AppenderRef ref="Console"/>
        </Root>
        <Root level="debug">
          <AppenderRef ref="Console"/>
        </Root>
        <Root level="trace">
          <AppenderRef ref="Console"/>
        </Root>
-->
  </Loggers>
</Configuration>